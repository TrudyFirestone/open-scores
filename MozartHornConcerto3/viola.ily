violaAllegro = \relative c
{
  \clef alto
  \key ees \major
  r4 |
  r8 ees-.\p ees( g) f( aes g f) |
  r g g g g g g g |
  f f bes bes bes bes bes bes |
  bes( ees) ees,-. ees-. ees4 r |
}

